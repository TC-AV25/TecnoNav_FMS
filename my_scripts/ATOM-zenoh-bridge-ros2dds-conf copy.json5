{
  mode: "peer",
  connect: {
    endpoints: [
      "tcp/172.30.10.52:7887",
      "tcp/212.161.77.248:7447", 
    ]
  },
  transport: {
    link: {
      tx: {
        queue:{
          /// Perform batching of messages if they are smaller of the batch_size
          batching: {
            /// Default works well in most cases. If performance problems arise, remove the comment to disable batching.
            // enabled: false,
          }
        }
      }
    }
  },
  plugins: {
    ros2dds: {
      allow: {
        publishers: [
          // not enable but Default
            "/sensing/camera/front_long/image_raw",
            "/control/command/actuation_cmd",
            
            // Send to bridge
            
            "/control/command/control_cmd",
            "/control/command/gear_cmd",
            "/control/current_gate_mode",
            "/control/command/turn_indicators_cmd",
            "/control/command/hazard_lights_cmd",

            // Used by FMS
            "/api/external/get/vehicle/status",
            "/api/external/get/cpu_usage",
            "/api/vehicle/kinematics",
            "/api/routing/route"
        ],
        subscribers: [
            //not enable but Default
            "/vehicle/status/actuation_status",
            // Receive from bridge
            
            "/vehicle/status/velocity_status",
            "/vehicle/status/steering_status",
            "/vehicle/status/control_mode",
            "/vehicle/status/gear_status",
            "/vehicle/status/turn_indicators_status",
            "/vehicle/status/hazard_lights_status",
            // Prevent cuda_blackboard subscriber negotiate to CUDA
            // "/sensing/lidar/.*",
            "/sensing/lidar/top/pointcloud",
            //"/sensing/imu/.*", because it is /sensing/gnss/imu 
            "/sensing/gnss/.*",
            "/clock",
            "/external/selected/gear_cmd",

            // Used by FMS
            "/api/external/set/command/remote/shift",
            "/external/selected/control_cmd",
            "/control/gate_mode_cmd",
            "/sensing/camera/.*"
        ],
        service_servers: [
            // Used by FMS
            "/api/operation_mode/change_to_autonomous",
            "/api/operation_mode/change_to_remote",
            "/api/routing/clear_route",
            "/api/routing/set_route_points"
        ],
        service_clients: [
            "/all_ignore"
        ],
        action_servers: [
            "/all_ignore"
        ],
        action_clients: [
            "/all_ignore"
        ],
      }
    }
  }
}
